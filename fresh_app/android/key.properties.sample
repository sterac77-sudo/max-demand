# Copy this file to fresh_app/android/key.properties and fill in values.
# Keep your real key.properties OUT of git (it's ignored in .gitignore).

storePassword=your-keystore-password
keyPassword=your-key-password
keyAlias=upload
# Path is resolved RELATIVE TO android/app/ directory (where build.gradle.kts uses file()).
# Since the JKS is at android/upload-keystore.jks, use:
storeFile=../upload-keystore.jks
